FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1.0;

boundaryField
{
    bottom
    {
        type fixedValue;
        value uniform 0.1;
    }
    top
    {
        type zeroGradient;
    }
    leftRight
    {
        type zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
}

