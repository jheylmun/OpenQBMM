FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

regions
(
    boxToCell
    {
        box (0 0 0) (0.5 0.45 0.1);
        fieldValues
        (
            volScalarFieldValue alpha.air   0
            volScalarFieldValue alpha.water 1
        );
    }
);


